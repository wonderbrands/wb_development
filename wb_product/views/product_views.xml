<odoo>
  <data>
    <record  id="product_product_form_inherit" model="ir.ui.view">

      <field name="name">product_product_inherit</field>
      <field name="model">product.product</field>
      <field name="inherit_id" ref="product.product_normal_form_view"/>
      <field name="arch" type="xml">
          <xpath expr="//group[@name='comercial']" position="after">
              <group string="Costos">
                  <field name="revised_cost" widget='monetary'/>
                  <!--<field name="previous_cost" widget='monetary'/> -->
              </group>
          </xpath>
          <xpath expr="//group[@name='status']" position="after">
              <group string="Stock">
                  <field name="stock_real"/>
                  <field name="stock_urrea"/>
                  <!--
                  <field name="stock_exclusivas"/>
                  <field name="stock_markets"/>
                  <field name="stock_supplier"/>
                  <field name="stock_mercadolibre"/>
                  <field name="stock_linio"/>
                  <field name="stock_amazon"/>
                  -->
              </group>
          </xpath>

      </field>
    </record>

  </data>
</odoo>